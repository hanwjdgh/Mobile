<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/colorButtonNormal"
    tools:context="org.androidtown.cok.Main2Activity">


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/tt"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/tt"
        android:layout_marginTop="69dp"
        android:text="   Project     : "
        android:textSize="24sp" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:layout_above="@+id/textView2"
        android:layout_alignLeft="@+id/finish"
        android:layout_alignStart="@+id/finish" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView"
        android:layout_alignStart="@+id/textView"
        android:layout_below="@+id/textView"
        android:layout_marginTop="40dp"
        android:text="   Meeting   : "
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignStart="@+id/textView2"
        android:layout_below="@+id/buttonm"
        android:layout_marginTop="31dp"
        android:text="    People    :  "
        android:textSize="24sp" />

    <TextView
        android:id="@+id/count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView2"
        android:layout_alignBottom="@+id/textView2"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp"
        android:layout_toEndOf="@+id/buttonm"
        android:layout_toRightOf="@+id/buttonm"
        android:text="0"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView9"
        android:layout_alignBottom="@+id/textView9"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp"
        android:layout_toEndOf="@+id/button2"
        android:layout_toRightOf="@+id/button2"
        android:text="0"
        android:textSize="30sp" />

    <Button
        android:id="@+id/buttonm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView2"
        android:layout_alignBottom="@+id/textView2"
        android:layout_alignLeft="@+id/editText3"
        android:layout_alignStart="@+id/editText3"
        android:background="@android:color/transparent"
        android:text="-"
        android:textSize="36sp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:text="-"
        android:textSize="36sp"
        android:layout_alignBaseline="@+id/textView9"
        android:layout_alignBottom="@+id/textView9"
        android:layout_alignLeft="@+id/buttonm"
        android:layout_alignStart="@+id/buttonm" />

    <Button
        android:id="@+id/buttonp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:text="+"
        android:textSize="30sp"
        android:layout_alignTop="@+id/buttonm"
        android:layout_alignRight="@+id/editText3"
        android:layout_alignEnd="@+id/editText3" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:text="+"
        android:textSize="30sp"
        android:layout_alignTop="@+id/button2"
        android:layout_alignLeft="@+id/buttonp"
        android:layout_alignStart="@+id/buttonp" />

    <Button
        android:id="@+id/finish"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="12dp"
        android:text="Done" />

    <Button
        android:id="@+id/fd_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/sd_dutton"
        android:layout_alignLeft="@+id/sd_dutton"
        android:layout_alignRight="@+id/sd_dutton"
        android:layout_alignStart="@+id/sd_dutton"
        android:layout_below="@+id/sd_dutton"
        android:layout_marginTop="49dp"
        android:background="@android:color/transparent"
        android:text="Button"
        android:textSize="24sp" />

    <Button
        android:id="@+id/sd_dutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/buttonp"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignRight="@+id/buttonp"
        android:layout_alignStart="@+id/textView2"
        android:layout_below="@+id/textView9"
        android:layout_marginTop="58dp"
        android:background="@android:color/transparent"
        android:text="Button"
        android:textSize="24sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tt"
        android:layout_above="@+id/finish"
        android:layout_toLeftOf="@+id/buttonm"
        android:layout_toStartOf="@+id/buttonm" />

</RelativeLayout>
